<%-include( 'partials/header' ) -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <label for="title">Title</label>
        <input type="text" name="title" autocomplete="off" class="title">
        <label for="post">Post</label>
        <textarea name="post" id="post" cols="102" rows="10"></textarea>
        <button class="btn publish-btn" type="submit">Publish</button>
    </form>
    <%-include( 'partials/footer' ) -%>